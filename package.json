{
    "name": "chord-identifier",
    "description": "Determine possible musical chord(s) from a guitar fretboard or collection of actual notes.",
    "version": "1.0.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "test": "tsc --project tsconfig.json && node tests/main.js",
        "prepare": "npm run build",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SeanMinchin/chord-identifier.git"
    },
    "keywords": [
        "chord",
        "guitar",
        "music",
        "chord identifier",
        "chord finder",
        "instrument",
        "reverse chord identifier",
        "reverse chord finder"
    ],
    "author": "Sean Minchin",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SeanMinchin/chord-identifier/issues"
    },
    "homepage": "https://github.com/SeanMinchin/chord-identifier/blob/master/README.md",
    "devDependencies": {
        "typescript": "^3.9.7"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    }
}
